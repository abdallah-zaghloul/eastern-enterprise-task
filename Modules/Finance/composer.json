{
    "name": "zaghloul-soft/finance-module",
    "description": "Financial Details about company stocks",
    "authors": [
        {
            "name": "Abdallah Zaghloul",
            "email": "3bdallahzaghloul@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Nwidart\\Modules\\LaravelModulesServiceProvider",
                "Prettus\\Repository\\Providers\\RepositoryServiceProvider"
            ],
            "aliases": {}
        },
        "module-dir": "Modules"
    },
    "autoload": {
        "psr-4": {
            "Modules\\Finance\\": "",
            "Modules\\Finance\\App\\": "app/",
            "Modules\\Finance\\Database\\Factories\\": "database/factories/",
            "Modules\\Finance\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Modules\\Finance\\Tests\\": "tests/"
        }
    },
    "require": {
        "nwidart/laravel-modules": "^10.0",
        "joshbrw/laravel-module-installer": "^2.0",
        "prettus/l5-repository": "^2.9",
        "laravel/ui": "^4.2",
        "livewire/livewire": "^3.1"
    },
    "require-dev": {
        "mhmiton/laravel-modules-livewire": "^2.1"
    },
    "config": {
        "allow-plugins": {
            "joshbrw/laravel-module-installer": true
        }
    }
}
